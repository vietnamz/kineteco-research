#!/Users/delgemoon/workspace/devops/files/kineteco-research/venv/bin/python3
# EASY-INSTALL-ENTRY-SCRIPT: 'pact-python==0.18.0','console_scripts','pact-verifier'
__requires__ = 'pact-python==0.18.0'
import re
import sys
from pkg_resources import load_entry_point

if __name__ == '__main__':
    sys.argv[0] = re.sub(r'(-script\.pyw?|\.exe)?$', '', sys.argv[0])
    sys.exit(
        load_entry_point('pact-python==0.18.0', 'console_scripts', 'pact-verifier')()
    )
